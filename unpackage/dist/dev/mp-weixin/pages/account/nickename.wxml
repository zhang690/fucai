<view class="namePull"><view class="pullTile"><view data-event-opts="{{[['tap',[['handleCancel',['$event']]]]]}}" class="cancel" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['handleFinish',['$event']]]]]}}" class="finish" bindtap="__e">完成</view></view><view class="pullInput"><input class="inputInfo" focus="{{isFocus}}" type="text" data-event-opts="{{[['input',[['__set_model',['','nickename','$event',[]]]]]]}}" value="{{nickename}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['handleClose',['$event']]]]]}}" class="close" bindtap="__e">x</view></view></view>