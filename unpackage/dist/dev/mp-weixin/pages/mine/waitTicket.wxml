<view class="waitTicket data-v-08aada34"><block wx:if="{{isDelete}}"><view class="order_number data-v-08aada34">订单编号：<view class="data-v-08aada34">{{detail.ID}}</view></view></block><view class="wait_container data-v-08aada34"><view class="wait_title data-v-08aada34"><view class="wait_title_left data-v-08aada34"><text class="data-v-08aada34">{{detail.Self_Delivery}}</text>{{detail.OutletsName+'投注站'}}</view><view class="wait_title_right data-v-08aada34">{{detail.name}}</view></view><view class="wait_list data-v-08aada34"><view class="wait_item data-v-08aada34"><view class="wait_item_icon data-v-08aada34"><image src="{{detail.icon}}" mode="widthFix" class="data-v-08aada34"></image></view><view class="wait_item_wrap data-v-08aada34"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="wait_item_info data-v-08aada34"><block wx:if="{{detail.LotteryType<4}}"><view class="qiu_text data-v-08aada34"><block wx:for="{{item.RedBall}}" wx:for-item="red" wx:for-index="__i0__" wx:key="*this"><text class="data-v-08aada34">{{red+(detail.LotteryType==4?' * '+item.Price/red+'元':'')}}</text></block><block wx:for="{{item.BlueBall}}" wx:for-item="blue" wx:for-index="__i1__" wx:key="*this"><text class="blue data-v-08aada34">{{blue}}</text></block></view></block><block wx:else><view class="qiu_text data-v-08aada34"><text class="data-v-08aada34">{{item.ProductName+" "+item.Price+"福豆/张"}}</text></view></block><block wx:if="{{detail.LotteryType<4}}"><view class="wait_item_qishu data-v-08aada34"><view class="qishu_text data-v-08aada34"><text class="data-v-08aada34">{{"期数*"+item.Num}}</text><text class="data-v-08aada34">{{"倍数*"+item.Multiple}}</text></view><view class="qishu_info data-v-08aada34">{{"第"+item.Fixed+"期"}}</view></view></block><block wx:else><view class="wait_item_qishu data-v-08aada34"><view class="qishu_text data-v-08aada34"><text class="data-v-08aada34">{{"共计："+item.Num+' 张'}}</text></view></view></block></view></block></view></view><view class="total_wrap data-v-08aada34"><view class="total_title data-v-08aada34"><view class="data-v-08aada34">{{"第 "+list[0].Number+" 期"}}</view><view class="data-v-08aada34">{{"共 "+list.length+" 注"}}</view></view><block wx:if="{{applyForDelete}}"><view data-event-opts="{{[['tap',[['deleteOrder',['$event']]]]]}}" class="total_delete data-v-08aada34" bindtap="__e">申请退单</view></block><block wx:else><view class="total_delete data-v-08aada34">{{applyForState}}</view></block><view class="total_item data-v-08aada34">{{"合计："+detail.Price+' 福豆'}}<block wx:if="{{detail.DeductionAmount>0}}"><text class="data-v-08aada34">{{"(优惠券:-"+detail.DeductionAmount+")"}}</text></block></view></view></view></view><view class="chupiao_container data-v-08aada34"><view class="chupiao_time data-v-08aada34">{{"下单时间："+detail.AddTime}}</view></view><block wx:if="{{applyForState===''}}"><block class="data-v-08aada34"><block wx:if="{{!isDelete}}"><view class="cuicu_btn data-v-08aada34"><view data-event-opts="{{[['tap',[['handleCuicu',['$event']]]]]}}" class="cuicu data-v-08aada34" bindtap="__e">催促兑票</view></view></block><block wx:if="{{!isDelete}}"><view class="chupiao data-v-08aada34"><view class="laod_chupiao data-v-08aada34">等待兑票...</view></view></block><block wx:if="{{isDelete}}"><view class="delete_area data-v-08aada34"><block wx:if="{{canDelete}}"><view data-event-opts="{{[['tap',[['beforeDelete',['$event']]]]]}}" class="delete_area_text data-v-08aada34" bindtap="__e">退单原因</view></block><view class="data-v-08aada34"><textarea placeholder="请输入您取消订单的原因" data-event-opts="{{[['blur',[['enterReason',['$event']]]]]}}" bindblur="__e" class="data-v-08aada34"></textarea></view></view></block><block wx:if="{{isDelete}}"><view class="footer data-v-08aada34"><view class="footer_btn_wrap data-v-08aada34"><view data-event-opts="{{[['tap',[['confirmDelete',['$event']]]]]}}" class="footer_btn data-v-08aada34" bindtap="__e">确认</view></view></view></block></block></block></view>