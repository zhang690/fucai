<view class="waitRecive data-v-fd2e7120"><block wx:if="{{isDelete}}"><view class="order_number data-v-fd2e7120">订单编号：<view class="data-v-fd2e7120">{{detail.ID}}</view></view></block><view class="wait_container data-v-fd2e7120"><view class="wait_title data-v-fd2e7120"><view class="wait_title_left data-v-fd2e7120"><text class="data-v-fd2e7120">{{detail.Self_Delivery}}</text>{{detail.OutletsName+'投注站'}}</view><view class="wait_title_right data-v-fd2e7120">{{detail.name}}</view></view><view class="wait_list data-v-fd2e7120"><view class="wait_item data-v-fd2e7120"><view class="wait_item_icon data-v-fd2e7120"><image src="{{detail.icon}}" mode="widthFix" class="data-v-fd2e7120"></image></view><view class="wait_item_wrap data-v-fd2e7120"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="wait_item_info data-v-fd2e7120"><view class="qiu_text data-v-fd2e7120"><block wx:for="{{item.RedBall}}" wx:for-item="red" wx:for-index="__i0__" wx:key="*this"><text class="data-v-fd2e7120">{{red+(detail.LotteryType==4?' * '+item.Price/red+'元':'')}}</text></block><block wx:for="{{item.BlueBall}}" wx:for-item="blue" wx:for-index="__i1__" wx:key="*this"><text class="blue data-v-fd2e7120">{{blue}}</text></block></view><block wx:if="{{detail.LotteryType<4}}"><view class="wait_item_qishu data-v-fd2e7120"><view class="qishu_text data-v-fd2e7120"><text class="data-v-fd2e7120">{{"期数*"+item.Num}}</text><text class="data-v-fd2e7120">{{"倍数*"+item.Multiple}}</text></view><view class="qishu_info data-v-fd2e7120">{{"第"+item.Fixed+"期"}}</view></view></block></view></block></view></view><view class="total_wrap data-v-fd2e7120"><view class="total_title data-v-fd2e7120"><view class="data-v-fd2e7120">{{"第 "+list[0].Number+" 期"}}</view><view class="data-v-fd2e7120">{{"共 "+list.length+" 注"}}</view></view><view data-event-opts="{{[['tap',[['deleteOrder',['$event']]]]]}}" class="total_delete data-v-fd2e7120" bindtap="__e">申请取消</view><view class="total_item data-v-fd2e7120">{{"合计："+detail.Price+' 福豆'}}<block wx:if="{{detail.DeductionAmount>0}}"><text class="data-v-fd2e7120">{{"(优惠券:-"+detail.DeductionAmount+")"}}</text></block></view></view></view></view><block wx:if="{{!isDelete}}"><view class="chupiao_container data-v-fd2e7120"><view class="chupiao_time data-v-fd2e7120">{{"下单时间："+detail.AddTime}}</view><view class="chupiao_time data-v-fd2e7120">{{"出票时间："+detail.LssueTime}}</view></view></block><block wx:if="{{!isDelete}}"><view class="chupiao data-v-fd2e7120"><view class="chupiao_title data-v-fd2e7120">兑票凭证</view><view class="chupiao_title_qrcode data-v-fd2e7120"><tki-qrcode vue-id="a89eb0d4-1" val="{{val}}" loadMake="{{loadMake}}" data-event-opts="{{[['^result',[['resultFn']]]]}}" bind:result="__e" class="data-v-fd2e7120" bind:__l="__l"></tki-qrcode></view><view class="laod_chupiao data-v-fd2e7120">等待兑票...</view><view class="chupiao_title_photo data-v-fd2e7120"><block wx:for="{{imgUrl}}" wx:for-item="v" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['previewImage',[i]]]]]}}" class="chupiao_title_photo_warp data-v-fd2e7120" bindtap="__e"><image src="{{v}}" mode="widthFix" class="data-v-fd2e7120"></image></view></block></view></view></block><block wx:if="{{isDelete}}"><view class="delete_area data-v-fd2e7120"><block wx:if="{{canDelete}}"><view data-event-opts="{{[['tap',[['beforeDelete',['$event']]]]]}}" class="delete_area_text data-v-fd2e7120" bindtap="__e">退单原因</view></block><view class="data-v-fd2e7120"><textarea placeholder="请输入您取消订单的原因" data-event-opts="{{[['blur',[['enterReason',['$event']]]]]}}" bindblur="__e" class="data-v-fd2e7120"></textarea></view></view></block><block wx:if="{{isDelete}}"><view class="footer data-v-fd2e7120"><view class="footer_btn_wrap data-v-fd2e7120"><view data-event-opts="{{[['tap',[['confirmDelete',['$event']]]]]}}" class="footer_btn data-v-fd2e7120" bindtap="__e">确认</view></view></view></block></view>